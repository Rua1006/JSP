<%
	List<String> lst = new ArrayList<>();
	lst.add("가");
	lst.add("나");
	lst.add("다");
	lst.add("라");
	lst.add("마");
	request.setAttribute("lst", lst);
	
	
	Set<String> set = new HashSet<>();
	set.add("바");
	set.add("사");
	set.add("아");
	set.add("자");
	request.setAttribute("set", set);
	
	
	Map map = new HashMap();
	map.put("test1","차");
	map.put("test2","카");
	map.put("test3","타");
	map.put("test4","파");
	request.setAttribute("map", map);
%>
